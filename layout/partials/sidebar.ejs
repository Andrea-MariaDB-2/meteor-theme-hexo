<div class="sidebar">
  <div class="panel">
    <div class="panel-item logo">
      <a href="https://developers.meteor.com" title="Meteor Developers">
        <%- partial('partials/icon-coral') %>
        <span>Meteor Developers</span>
      </a>
    </div>
    <div class="panel-item">
      <a class="" href="http://tutorials.meteor.com">
        <%- partial('partials/illustration-guide') %>
        <span>Tutorials</span>
      </a>
    </div>

    <div class="panel-item">
      <a class="" href="http://guide.meteor.com">
        <%- partial('partials/illustration-compass') %>
        <span>Guide</span>
      </a>
    </div>

    <div class="panel-item active">
      <a class="" href="http://docs.meteor.com">
        <%- partial('partials/illustration-logs') %>
        <span>API Docs</span>
      </a>
    </div>

    <div class="panel-item">
      <a class="" href="http://forums.meteor.com/">
        <%- partial('partials/illustration-support') %>
        <span>Forums</span>
      </a>
    </div>
  </div>

